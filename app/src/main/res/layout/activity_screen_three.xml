            android:id="@+id/view4"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/text_size_xlarge"
            android:background="@color/grayColor"
            app:layout_constraintTop_toBottomOf="@+id/materialTextView27" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/materialTextView28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/text_size_xlarge"
            android:text="Cancel this booking"
            android:textAppearance="?attr/textAppearanceBody1"
            android:textSize="@dimen/text_size_medium"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/materialTextView12"
            app:layout_constraintTop_toTopOf="@+id/view4" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_icohelpcancel"
            app:layout_constraintBottom_toBottomOf="@+id/materialTextView28"
            app:layout_constraintEnd_toEndOf="@+id/imageView5"
            app:layout_constraintTop_toTopOf="@+id/materialTextView28" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/text_size_xlarge"
            android:background="@color/grayColor"
            app:layout_constraintTop_toBottomOf="@+id/materialTextView28" />

    </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</FrameLayout>